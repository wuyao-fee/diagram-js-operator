{
  "name": "diagram-js-operator",
  "version": "1.0.1",
  "description": "基于diagram-js/bpmn-js的BPMN操作栏插件",
  "type": "module",
  "private": false,
  "main": "dist/diagram-js-operator.cjs.js",
  "module": "dist/diagram-js-operator.esm.js",
  "umd:main": "dist/diagram-js-operator.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "demo": "vite ./demo",
    "build": "rollup -c && sass src/index.scss dist/diagram-js-operator.css --style=compressed",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "check-format": "prettier --check \"src/**/*.{ts,js,json,md}\""
  },
  "keywords": [
    "bpmn-js-operator",
    "diagram-js",
    "bpmn-js",
    "operator",
    "bpmn"
  ],
  "author": "wuyao",
  "email": "wuyao.fee@qq.com",
  "homepage": "https://gitee.com/wuyao-fee/diagram-js-operator",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/wuyao-fee/diagram-js-operator.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "bpmn-js": "^18.9.1",
    "diagram-js": "*",
    "diagram-js-minimap": "^5.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "sass": "^1.97.1",
    "typescript": "^5.9.3",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "bpmn-js": "^18.9.1",
    "diagram-js": "^15.4.0",
    "diagram-js-minimap": "^5.2.0"
  },
  "dependencies": {
    "min-dom": "^5.1.2",
    "tslib": "^2.8.1"
  },
  "engines": {
    "node": ">=18.0.0 <20.0.0"
  }
}
